<h1>Listing orders</h1>
<%= will_paginate @orders %>
<table>
  <tr>
    <th>Quantity</th>
    <th>Payment</th>
	 <th>Ride</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @orders.each do |order| %>
  <tr>
    <td><%= order.quantity %></td>
    <td><%= order.payment %></td>
	<td><%= order.ride_id %></td>
    <td><%= order.user_id %></td>
    <td><%= link_to 'Show', order %></td>
  </tr>
<% end %>
</table>

<br />

